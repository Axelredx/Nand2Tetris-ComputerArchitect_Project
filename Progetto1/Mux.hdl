/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Not(in=sel, out=Nsel);
	And(a=a, b=Nsel, out=aNsel);
	And(a=sel, b=b, out=selb);
	Or(a=aNsel, b=selb, out=out);
}